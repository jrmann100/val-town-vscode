{
	"name": "valtown",
	"displayName": "Val Town",
	"description": "VS Code integration for val.town",
	"version": "0.4.1",
	"publisher": "pomdtr",
	"private": true,
	"icon": "./assets/icon.png",
	"license": "MIT",
	"homepage": "https://val.town",
	"repository": {
		"type": "git",
		"url": "https://github.com/pomdtr/valtown-vscode"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Education",
		"Other"
	],
	"activationEvents": [
		"onFileSystem:vt+val",
		"onFileSystem:vt+blob"
	],
	"browser": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "valtown",
					"title": "Val Town",
					"icon": "resources/activity.svg"
				}
			]
		},
		"views": {
			"valtown": [
				{
					"id": "valtown.vals",
					"name": "Vals"
				},
				{
					"id": "valtown.blobs",
					"name": "Blobs"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "valtown.vals",
				"contents": "Set your Val Town token to use this extension.\n[Set Token](command:valtown.setToken)\n[API Tokens](https://www.val.town/settings/api)",
				"when": "!valtown.loggedIn"
			},
			{
				"view": "valtown.blobs",
				"contents": "Set your Val Town token to use this extension.\n[Set Token](command:valtown.setToken)\n[API Tokens](https://www.val.town/settings/api)",
				"when": "!valtown.loggedIn"
			}
		],
		"commands": [
			{
				"command": "valtown.createVal",
				"title": "Create Val",
				"category": "Val Town",
				"icon": "$(plus)"
			},
			{
				"command": "valtown.createValFromTemplate",
				"title": "Create Val from Template",
				"category": "Val Town",
				"icon": "$(new-file)"
			},
			{
				"command": "valtown.deleteVal",
				"title": "Delete Val",
				"category": "Val Town",
				"icon": "$(trash)"
			},
			{
				"command": "valtown.openReadme",
				"title": "Open Readme",
				"category": "Val Town",
				"icon": "$(book)"
			},
			{
				"command": "valtown.openReadmeToSide",
				"title": "Open Readme to the Side",
				"category": "Val Town",
				"icon": "$(open-preview)"
			},
			{
				"command": "valtown.refresh",
				"title": "Refresh View",
				"category": "Val Town",
				"icon": "$(refresh)"
			},
			{
				"command": "valtown.openInBrowser",
				"title": "Open Val in Browser",
				"category": "Val Town",
				"icon": "$(globe)"
			},
			{
				"command": "valtown.open",
				"title": "Open Val",
				"category": "Val Town",
				"icon": "$(file-code)"
			},
			{
				"command": "valtown.rename",
				"title": "Rename Val",
				"category": "Val Town",
				"icon": "$(edit)"
			},
			{
				"command": "valtown.setPrivacy",
				"title": "Set Privacy",
				"category": "Val Town",
				"icon": "$(eye)"
			},
			{
				"command": "valtown.setPrivate",
				"title": "Private",
				"category": "Val Town"
			},
			{
				"command": "valtown.setUnlisted",
				"title": "Unlisted",
				"category": "Val Town"
			},
			{
				"command": "valtown.setPublic",
				"title": "Public",
				"category": "Val Town"
			},
			{
				"command": "valtown.quickOpen",
				"title": "Open Val...",
				"category": "Val Town",
				"icon": "$(file-code)"
			},
			{
				"command": "valtown.blob.quickOpen",
				"title": "Open Blob...",
				"category": "Val Town"
			},
			{
				"command": "copyValID",
				"title": "Val ID",
				"category": "Val Town"
			},
			{
				"command": "valtown.copyModuleURL",
				"title": "Module URL",
				"category": "Val Town"
			},
			{
				"command": "valtown.copyScriptTag",
				"title": "Script Tag",
				"category": "Val Town"
			},
			{
				"command": "valtown.copyValUrl",
				"title": "Val URL",
				"category": "Val Town",
				"icon": "$(link)"
			},
			{
				"command": "valtown.copyEmbedUrl",
				"title": "Embed URL",
				"category": "Val Town"
			},
			{
				"command": "valtown.copyEmailAddress",
				"title": "Email Address",
				"category": "Val Town"
			},
			{
				"command": "valtown.copyHttpEndpoint",
				"title": "HTTP Endpoint",
				"category": "Val Town"
			},
			{
				"command": "valtown.openValUrl",
				"title": "Val URL",
				"category": "Val Town"
			},
			{
				"command": "valtown.openHttpEndpoint",
				"title": "HTTP Endpoint",
				"category": "Val Town"
			},
			{
				"command": "valtown.setToken",
				"category": "Val Town",
				"title": "Set Token"
			},
			{
				"command": "valtown.clearToken",
				"category": "Val Town",
				"title": "Clear Token"
			},
			{
				"command": "valtown.openValLogs",
				"category": "Val Town",
				"title": "Val Logs"
			},
			{
				"command": "valtown.togglePinned",
				"category": "Val Town",
				"title": "Toggle Pinned"
			},
			{
				"command": "valtown.blob.refresh",
				"category": "Val Town",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "valtown.blob.delete",
				"category": "Val Town",
				"title": "Delete"
			},
			{
				"command": "valtown.blob.upload",
				"category": "Val Town",
				"title": "Upload Blob",
				"icon": "$(cloud-upload)"
			},
			{
				"command": "valtown.blob.download",
				"category": "Val Town",
				"title": "Download",
				"icon": "$(cloud-download)"
			},
			{
				"command": "valtown.blob.openInNewWindow",
				"category": "Val Town",
				"title": "Open in New Window",
				"icon": "$(empty-window)"
			}
		],
		"configuration": [
			{
				"title": "Val Town",
				"type": "object",
				"properties": {
					"valtown.endpoint": {
						"type": "string",
						"default": "https://api.val.town"
					}
				}
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "valtown.setToken",
					"when": "!valtown.loggedIn"
				},
				{
					"command": "valtown.clearToken",
					"when": "valtown.loggedIn"
				},
				{
					"command": "valtown.quickOpen",
					"when": "valtown.loggedIn"
				},
				{
					"command": "valtown.createVal",
					"when": "valtown.loggedIn"
				},
				{
					"command": "valtown.createValFromTemplate",
					"when": "valtown.loggedIn"
				},
				{
					"command": "valtown.deleteVal",
					"when": "false"
				},
				{
					"command": "valtown.openReadme",
					"when": "false"
				},
				{
					"command": "valtown.openReadmeToSide",
					"when": "false"
				},
				{
					"command": "valtown.refresh",
					"when": "false"
				},
				{
					"command": "valtown.openInBrowser",
					"when": "false"
				},
				{
					"command": "valtown.open",
					"when": "false"
				},
				{
					"command": "valtown.setPrivacy",
					"when": "false"
				},
				{
					"command": "valtown.setPrivate",
					"when": "false"
				},
				{
					"command": "valtown.setUnlisted",
					"when": "false"
				},
				{
					"command": "valtown.setPublic",
					"when": "false"
				},
				{
					"command": "copyValID",
					"when": "false"
				},
				{
					"command": "valtown.copyModuleURL",
					"when": "false"
				},
				{
					"command": "valtown.copyScriptTag",
					"when": "false"
				},
				{
					"command": "valtown.copyValUrl",
					"when": "false"
				},
				{
					"command": "valtown.copyEmbedUrl",
					"when": "false"
				},
				{
					"command": "valtown.copyEmailAddress",
					"when": "false"
				},
				{
					"command": "valtown.copyHttpEndpoint",
					"when": "false"
				},
				{
					"command": "valtown.openValUrl",
					"when": "false"
				},
				{
					"command": "valtown.openHttpEndpoint",
					"when": "false"
				},
				{
					"command": "valtown.openValLogs",
					"when": "false"
				},
				{
					"command": "valtown.togglePinned",
					"when": "false"
				},
				{
					"command": "valtown.blob.refresh",
					"when": "false"
				},
				{
					"command": "valtown.blob.delete",
					"when": "false"
				},
				{
					"command": "valtown.blob.download",
					"when": "false"
				},
				{
					"command": "valtown.blob.openInNewWindow",
					"when": "false"
				}
			],
			"editor/title": [
				{
					"command": "valtown.openReadmeToSide",
					"alt": "valtown.openReadme",
					"group": "navigation@1",
					"when": "resourceScheme == 'vt+val' && resourceLangId != markdown"
				}
			],
			"view/title": [
				{
					"command": "valtown.createVal",
					"group": "navigation@1",
					"when": "view == valtown.vals && valtown.loggedIn"
				},
				{
					"command": "valtown.createValFromTemplate",
					"group": "navigation@2",
					"when": "view == valtown.vals && valtown.loggedIn"
				},
				{
					"command": "valtown.refresh",
					"group": "navigation@3",
					"when": "view == valtown.vals && valtown.loggedIn"
				},
				{
					"command": "valtown.blob.upload",
					"group": "navigation@1",
					"when": "view == valtown.blobs && valtown.loggedIn"
				},
				{
					"command": "valtown.blob.openInNewWindow",
					"group": "navigation@2",
					"when": "view == valtown.blobs && valtown.loggedIn"
				},
				{
					"command": "valtown.blob.refresh",
					"group": "navigation@3",
					"when": "view == valtown.blobs && valtown.loggedIn"
				}
			],
			"view/item/context": [
				{
					"command": "valtown.rename",
					"group": "navigation@1",
					"when": "viewItem == val"
				},
				{
					"submenu": "valtown.submenus.copy",
					"group": "navigation@2",
					"when": "viewItem == val"
				},
				{
					"submenu": "valtown.submenus.open",
					"group": "navigation@3",
					"when": "viewItem == val"
				},
				{
					"submenu": "valtown.submenus.privacy",
					"group": "navigation@4",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.togglePinned",
					"group": "navigation@5",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.deleteVal",
					"group": "navigation@6",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.blob.download",
					"group": "navigation@1",
					"when": "viewItem == blob"
				},
				{
					"command": "valtown.blob.delete",
					"group": "navigation@2",
					"when": "viewItem == blob"
				}
			],
			"valtown.submenus.privacy": [
				{
					"command": "valtown.setPrivate",
					"group": "navigation@2",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.setUnlisted",
					"group": "navigation@3",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.setPublic",
					"group": "navigation@1",
					"when": "viewItem == val"
				}
			],
			"valtown.submenus.open": [
				{
					"command": "valtown.openValUrl",
					"group": "navigation@1",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.openHttpEndpoint",
					"group": "navigation@2",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.openValLogs",
					"group": "navigation@3",
					"when": "viewItem == val"
				}
			],
			"valtown.submenus.copy": [
				{
					"command": "valtown.copyValUrl",
					"group": "navigation@1",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.copyModuleURL",
					"group": "navigation@2",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.copyScriptTag",
					"group": "navigation@3",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.copyEmbedUrl",
					"group": "navigation@4",
					"when": "viewItem == val"
				},
				{
					"command": "copyValID",
					"group": "navigation@5",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.copyHttpEndpoint",
					"group": "navigation@6",
					"when": "viewItem == val"
				},
				{
					"command": "valtown.copyEmailAddress",
					"group": "navigation@7",
					"when": "viewItem == val"
				}
			]
		},
		"submenus": [
			{
				"id": "valtown.submenus.open",
				"label": "Open"
			},
			{
				"id": "valtown.submenus.privacy",
				"label": "Set Privacy"
			},
			{
				"id": "valtown.submenus.copy",
				"label": "Copy"
			},
			{
				"id": "valtown.privacy",
				"label": "Set Privacy"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext=ts"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"ts-loader": "^9.5.0",
		"typescript": "^5.2.2",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4"
	}
}
